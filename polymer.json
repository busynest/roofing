{
    "entrypoint": "index.html",
    "shell": "src/application-shell.js",
    "npm": true,
    "moduleResolution": "node",
    "fragments": [
      "src/app/metadata.js",
      "src/app/result-item.js",
      "src/app/router.js",
      "src/page-404.js",
      "src/primary-contract.js",
      "src/purchase-order.js",
      "src/send-feedback.js",
      "src/sub-contract.js",
      "src/warranty-contract.js"
    ],
  
    "sources": [
      "src/**",
      "images/**",
      "package.json"
    ],
  
    "extraDependencies": [
      "manifest.json",
      "node_modules/@webcomponents/webcomponentsjs/**"
    ],
  
    "builds": [{
      "preset": "es6-unbundled",
      "addServiceWorker": true,
      "js": {"compile": true, "minify": true},
      "css": {"minify": true},
      "html": {"minify": true}
    }]
  }