{
    "entrypoint": "index.html",
    "shell": "src/application-shell.js",
    "npm": true,
    "moduleResolution": "node",
    "fragments": [
      "src/404.js",
      "src/menu-topic.js",
      "src/metadata.js",
      "src/primary-contract.js",
      "src/purchase-order.js",
      "src/result-item.js",
      "src/send-feedback.js",
      "src/sub-contract.js",
      "src/warranty-contract.js"
    ],
  
    "sources": [
      "src/**",
      "images/**"
    ],
  
    "extraDependencies": [
      "manifest.json",
      "bower_components/webcomponentsjs/*.js"
    ],
  
    "builds": [{
      "preset": "es6-unbundled",
      "addServiceWorker": true,
      "js": {"compile": true, "minify": true},
      "css": {"minify": true},
      "html": {"minify": true},
      "addServiceWorker": true
    }]
  }